<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.customerui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".paytm_payment"></activity>
        <activity android:name=".payment_activity" />
        <activity android:name=".new_order" />
        <activity android:name=".active_order" />
        <activity android:name=".order_history" />
        <activity android:name=".dashboard" />
        <activity android:name=".login_activity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="Customer" />
        <activity android:name=".my_profile" />
        <activity android:name=".verification_otp" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>